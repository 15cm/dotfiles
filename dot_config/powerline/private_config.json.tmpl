{
  "ext": {
    "shell": {
      "theme": "default",
      "local_themes": {
        "continuation": "continuation",
        "select": "select"
      },
      {{if (eq .global.theme "light")}}
      "colorscheme": "solarized-light"
      {{else}}
      "colorscheme": "nord"
      {{end}}
    },
    "wm": {
      "theme": "default",
      "colorscheme": "default"
    },
    "tmux": {
      "theme": "default",
      {{if (eq .global.theme "light")}}
      "colorscheme": "solarized-light"
      {{else}}
      "colorscheme": "nord"
      {{end}}
    },
    "vim": {
      "theme": "default",
      "local_themes": {
        "powerline.matchers.vim.plugin.gundo.gundo_preview": "plugin_gundo-preview",
        "cmdwin": "cmdwin",
        "help": "help",
        "__tabline__": "tabline",
        "powerline.matchers.vim.plugin.nerdtree.nerdtree": "plugin_nerdtree",
        "powerline.matchers.vim.plugin.commandt.commandt": "plugin_commandt",
        "powerline.matchers.vim.plugin.gundo.gundo": "plugin_gundo",
        "quickfix": "quickfix"
      },
      "colorscheme": "default"
    },
    "ipython": {
      "theme": "in",
      "local_themes": {
        "rewrite": "rewrite",
        "in2": "in2",
        "out": "out"
      },
      "colorscheme": "default"
    },
    "pdb": {
      "theme": "default",
      "colorscheme": "default"
    }
  },
  "common": {
    "term_truecolor": false
  }
}
